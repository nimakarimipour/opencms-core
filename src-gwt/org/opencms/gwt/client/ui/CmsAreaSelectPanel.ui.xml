<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field="style" type="org.opencms.gwt.client.ui.css.I_CmsLayoutBundle" />
	<g:HTMLPanel styleName="{style.selectAreaCss.main}">
				<g:FlowPanel ui:field="m_panel" />
				<div ui:field="m_overlayTop" class="{style.selectAreaCss.overlay}" style="top: 0px; left: 0px;" />
				<div ui:field="m_overlayRight" class="{style.selectAreaCss.overlay}" style="right: 0px; top: 0px; bottom: 0px;" />
				<div ui:field="m_overlayBottom" class="{style.selectAreaCss.overlay}" style="bottom: 0px; left: 0px;" />
				<div ui:field="m_overlayLeft" class="{style.selectAreaCss.overlay}" style="top: 0px; left: 0px; bottom: 0px;" />
				<div ui:field="m_marker" class="{style.selectAreaCss.marker}"><div class="{style.selectAreaCss.markerBlackBorder}" /><div class="{style.selectAreaCss.markerWhiteBorder}" /></div>
	</g:HTMLPanel>
</ui:UiBinder> 